package fsm_2;

import fsm.EvaluarPropuestaBehaviour;
import fsm.MCPBehaviour;
import jade.core.behaviours.Behaviour;
import jade.lang.acl.ACLMessage;

public class AcuerdoBehaviour extends Behaviour {

	@Override
	public void action() {
		
		ACLMessage pelicula_propuesta = (ACLMessage)getDataStore().get(MCPBehaviour.PROPUESTA);
		System.out.println("Se ha llegado a un acuerdo, pelicula = " + EvaluarPropuestaBehaviour.FINAL_MOVIE );
	}

	@Override
	public boolean done() {
		return true;
	}

}


 